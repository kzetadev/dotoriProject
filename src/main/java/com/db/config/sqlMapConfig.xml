<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
<properties resource="com/db/config/db.properties"/>
<typeAliases>
	<typeAlias type="com.board.vo.Board_PostVo" alias="Board_PostVo"/>
	<typeAlias type="com.member.vo.Member_InfoVo" alias="Member_InfoVo"/>
	<typeAlias type="com.information.vo.Place_ThemeVo" alias="Place_ThemeVo"/>
	<typeAlias type="com.information.vo.Place_InfoVo" alias="Place_InfoVo"/>
	<typeAlias type="com.information.vo.Member_FavoriteVo" alias="Member_FavoriteVo"/>
	<typeAlias type="com.board.vo.Head_TagVo" alias="Head_TagVo"/>
</typeAliases>
  <environments default="development">
    <environment id="development">
      <transactionManager type="JDBC"/>
      <dataSource type="POOLED">
        <property name="driver" value="${driver}"/>
        <property name="url" value="${url}"/>
        <property name="username" value="${username}"/>
        <property name="password" value="${password}"/>
      </dataSource>
    </environment>
  </environments>
  <mappers>
    <mapper resource="com/board/mapper/Board_PostMapper.xml"/>
    <mapper resource="com/member/mapper/member_InfoMapper.xml"/>
    <mapper resource="com/information/mapper/Place_InfoMapper.xml"/>
    <mapper resource="com/information/mapper/Place_ThemeMapper.xml"/>
    <mapper resource="com/information/mapper/Member_FavoriteMapper.xml"/>
    <mapper resource="com/member/mapper/AdminMapper.xml"/>
  </mappers>
</configuration>